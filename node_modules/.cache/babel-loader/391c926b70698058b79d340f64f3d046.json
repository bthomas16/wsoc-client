T_API\n    };\n  },\n  methods: {\n    submitEditProfile: function submitEditProfile() {\n      var _this = this;\n\n      this.isShowErrorMessage = false;\n      this.$store.dispatch('editUserProfile', this.userProfileEditing).then(function (data) {\n        // edit profile\n        if (data.isSuccess) {\n          _this.$refs.editProfileModal.hide();\n\n          _this.$store.dispatch('user'); // reload user\n\n        } else {\n          _this.errorObj.isSuccess = data.isSuccess;\n          _this.errorObj.message = data.message;\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    loaded: function loaded() {\n      this.isLoaded = true;\n    }\n  },\n  computed: {\n    User: function User() {\n      var u = this.$store.state.User;\n      this.userProfileEditing = u;\n      this.userProfileEditing.oldPassword;\n      this.userProfileEditing.newPassword;\n      this.userProfileEditing.confirmNewPassword;\n      return u;\n    },\n    Collection: function Collection() {\n      var length = this.$store.state.Collection.length;\n\n      if (this.$store.state.Collection.length > 0) {\n        return length;\n      } else return 0;\n    },\n    watchesFSOT: function watchesFSOT() {\n      return this.$store.state.NumberFSOT;\n    },\n    Favorites: function Favorites() {\n      return this.$store.state.Favorites.length;\n    } // errorObj() {\n    //     return {\n    //         isSuccess: true,\n    //         message: 'Test'\n    //     }\n    // }\n\n  },\n  created: function created() {\n    this.$store.dispatch('getNumberFSOT');\n  }\n};",{"version":3,"sources":["ProfileInfo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2DA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,WAAA,MAAA,0CAAA;AAEA,eAAA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,WAAA,EAAA;AADA,GAFA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,kBAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA;AACA,QAAA,SAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAFA;AAMA,MAAA,kBAAA,EAAA,KANA;AAOA,MAAA,QAAA,EAAA,KAPA;AAQA,MAAA,QAAA,EAAA,OAAA,CAAA,GAAA,CAAA;AARA,KAAA;AAUA,GAjBA;AAkBA,EAAA,OAAA,EAAA;AACA,IAAA,iBADA,+BACA;AAAA;;AACA,WAAA,kBAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,EAAA,KAAA,kBAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AAAA;AACA,YAAA,IAAA,CAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,gBAAA,CAAA,IAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,MAAA,EAFA,CAEA;;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,SAAA,GAAA,IAAA,CAAA,SAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA;AACA,OARA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA;AAWA,KAdA;AAgBA,IAAA,MAhBA,oBAgBA;AACA,WAAA,QAAA,GAAA,IAAA;AACA;AAlBA,GAlBA;AAsCA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,UAAA,CAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA;AACA,WAAA,kBAAA,GAAA,CAAA;AACA,WAAA,kBAAA,CAAA,WAAA;AACA,WAAA,kBAAA,CAAA,WAAA;AACA,WAAA,kBAAA,CAAA,kBAAA;AACA,aAAA,CAAA;AACA,KARA;AAUA,IAAA,UAVA,wBAUA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,MAAA;;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,MAAA;AACA,OAFA,MAEA,OAAA,CAAA;AACA,KAfA;AAiBA,IAAA,WAjBA,yBAiBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,UAAA;AACA,KAnBA;AAqBA,IAAA,SArBA,uBAqBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,MAAA;AACA,KAvBA,CAyBA;AACA;AACA;AACA;AACA;AACA;;AA9BA,GAtCA;AAuEA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,eAAA;AACA;AAzEA,CAAA","sourcesContent":["<template>\n    <b-container>\n        <b-row align-v=\"center\" align-h=\"center\" class=\"py-3 relative\">\n            <b-col cols=\"4\" offset=\"8\" class=\"pointer relative settingIconWrapper\">\n                <b-img fluid :src=\"ROOT_API + '/api/static-assets/editIcon.png'\" v-b-modal.editProfileModal id=\"settingIcon\" class=\"mr-2 absolute t-0 right-align\"></b-img>\n            </b-col>\n            <b-col cols=\"5\" sm=\"4\" md=\"12\" class=\"m-0 mx-auto center p-md-2 pt-lg-3\">\n\n                <b-img v-if=\"!isLoaded\" :src=\"User.imgSrc || ROOT_API + '/api/static-assets/blankprofpic.png'\" fluid style=\"height: auto; max-height: 125px;\" class=\"profPic mx-auto box-shadow-light\" rounded></b-img>\n                <div id=\"loader\" class=\"h-100\" v-if=\"isLoaded\">Loading...</div>\n\n            </b-col>\n            <b-col cols=\"7\" sm=\"7\" md=\"12\" class=\"left mx-auto p-0 pl-2 mt-2\" >\n                <p class=\"p-0 my-1 ml-1 h5 m-h2 bold\"><strong>{{User.firstName}} {{User.lastName}}</strong></p>\n                <p class=\"p-0 my-1 mt-2 ml-1 m-h2 h7\"><strong>({{Collection}})</strong> Watches Total</p>\n                <p class=\"p-0 my-1 ml-1 m-h2 h7\"><strong>({{watchesFSOT}})</strong> Watches FSOT</p>\n                <p class=\"p-0 my-1 ml-1 m-h2 h7\"><strong>({{Favorites || 0}})</strong> Watches Favorited</p>\n            </b-col>\n\n            <!-- // Edit Modal -->\n            <b-row no-gutters>\n                <b-modal id=\"editProfileModal\" ref=\"editProfileModal\">\n                    <div slot=\"modal-title\">\n                        Editing: {{userProfileEditing.firstName}}\n                    </div>\n                    <edit-profile :userProfileEditing=\"userProfileEditing\" :errorObj=\"errorObj\"></edit-profile>\n                    <div slot=\"modal-footer\" class=\"w-100 mt-0 p-0\">\n                        <b-row no-gutters align-v=\"center\">\n                            <b-col cols=\"9\" class=\"mt-3\" v-if=\"userProfileEditing.newPassword && userProfileEditing.confirmNewPassword\">\n                                <p class=\"red m-h3 nowrap\" v-if=\"!userProfileEditing.email\">Must provide an email address</p>\n                                <p class=\"red m-h3 nowrap\" v-if=\"!userProfileEditing.firstName\">Must provide a first name</p>\n                                <p class=\"red m-h3 nowrap\" v-if=\"userProfileEditing.newPassword.length < 4 && userProfileEditing.newPassword != ''\">Password must have 4 characters</p>\n                                <p class=\"red m-h3 nowrap\" v-if=\"(userProfileEditing.newPassword != userProfileEditing.confirmNewPassword) && (userProfileEditing.newPassword.length >= 4)\">New Password does not match</p>\n\n                                <p class=\"blue m-h3 nowrap\" v-if=\"userProfileEditing.email && userProfileEditing.firstName && (userProfileEditing.newPassword.length >= 4) && (userProfileEditing.newPassword == userProfileEditing.confirmNewPassword) && (userProfileEditing.oldPassword) && (userProfileEditing.newPassword)\">Ready to Submit!</p>\n\n                                <p class=\"red m-h3 nowrap\" v-if=\"(userProfileEditing.newPassword.length >= 4 ) && (userProfileEditing.confirmNewPassword.length >= 4) && (!userProfileEditing.oldPassword) && (userProfileEditing.newPassword)\">Please provide your old password</p>\n                            </b-col>\n                            <b-col>\n                                <input class=\"float-right btn bg-green white\" variant=\"info\" type=\"submit\" value=\"Submit\"\n                                    @click=\"submitEditProfile\"\n                                    :disabled=\"\n                                    (!userProfileEditing.email) || (!userProfileEditing.firstName) ||\n                                    (userProfileEditing.newPassword != userProfileEditing.confirmNewPassword) ||\n                                    (\n                                        (userProfileEditing.oldPassword == '') && (userProfileEditing.newPassword != '')\n                                    )\"\n                                />\n                            </b-col>\n                        </b-row>\n                    </div>\n                </b-modal>\n            </b-row>\n\n        </b-row>\n    </b-container>\n</template>\n\n<script>\nimport axios from 'axios'\nimport EditProfile from './Collection/Modals/EditProfileModal.vue'\n\nexport default {\n\n  components: {\n    editProfile: EditProfile\n  },\n\n  data () {\n    return {\n      userProfileEditing: {},\n      errorObj: {\n        isSuccess: true,\n        message: ''\n      },\n      isShowErrorMessage: false,\n      isLoaded: false,\n      ROOT_API: process.env.VUE_APP_ROOT_API\n    }\n  },\n  methods: {\n    submitEditProfile () {\n      this.isShowErrorMessage = false\n      this.$store.dispatch('editUserProfile', this.userProfileEditing).then((data) => { // edit profile\n        if (data.isSuccess) {\n          this.$refs.editProfileModal.hide()\n          this.$store.dispatch('user') // reload user\n        } else {\n          this.errorObj.isSuccess = data.isSuccess\n          this.errorObj.message = data.message\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n\n    loaded () {\n      this.isLoaded = true\n    }\n  },\n  computed: {\n    User () {\n      let u = this.$store.state.User\n      this.userProfileEditing = u\n      this.userProfileEditing.oldPassword\n      this.userProfileEditing.newPassword\n      this.userProfileEditing.confirmNewPassword\n      return u\n    },\n\n    Collection () {\n      let length = this.$store.state.Collection.length\n      if (this.$store.state.Collection.length > 0) {\n        return length\n      } else return 0\n    },\n\n    watchesFSOT () {\n      return this.$store.state.NumberFSOT\n    },\n\n    Favorites () {\n      return this.$store.state.Favorites.length\n    }\n\n    // errorObj() {\n    //     return {\n    //         isSuccess: true,\n    //         message: 'Test'\n    //     }\n    // }\n  },\n\n  created: function () {\n    this.$store.dispatch('getNumberFSOT')\n  }\n}\n</script>\n\n<style scoped>\n\n.profPic {\n    border: 2px solid lightgray;\n}\n\n#settingIcon{\n    right: 0;\n    top: 0;\n    width: 1.15em;\n}\n\n.settingIconWrapper {\n        height: .75rem;\n    }\n\n@media(max-width:800px){\n    .settingIconWrapper {\n        height: .15rem;\n    }\n}\n\n@media(max-width:350px){\n    .profPic {\n        height:140px;\n    }\n\n}\n\n#loader {\n    height:10rem !important;\n}\n\n</style>\n"],"sourceRoot":"src/components/Profile"}]}