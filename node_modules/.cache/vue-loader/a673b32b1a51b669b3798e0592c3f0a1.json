ontent: 'https://www.my-site.com/my-special-image.jpg'},\n        // Often the same as your meta description, but not always.\n        {property: 'og:description', content: 'Manage your current, past and future Watch SOC. Add, Edit and remove watches from your collection while tracking their values, spencs, condition and styles along the way.'},\n\n        // Twitter card\n        {name: 'twitter:card', content: 'Manage your current, past and future Watch SOC.'},\n        {name: 'twitter:site', content: process.env.BASE_URL},\n        {name: 'twitter:title', content: 'Watch SOC (State Of The Collection)'},\n        {name: 'twitter:description', content: 'Manage your current, past and future Watch SOC. Add, Edit and remove watches from your collection while tracking their values, spencs, condition and styles along the way.'},\n        // Your twitter handle, if you have one.\n        // {name: 'twitter:creator', content: '@alligatorio'},\n        // {name: 'twitter:image:src', content: 'https://www.my-site.com/my-special-image.jpg'},\n\n        // Google / Schema.org markup:\n        {itemprop: 'name', content: 'Watch SOC (State Of The Collection)'},\n        {itemprop: 'description', content: 'Manage your current, past and future Watch SOC. Add, Edit and remove watches from your collection while tracking their values, spencs, condition and styles along the way.'},\n        // {itemprop: 'image', content: 'https://www.my-site.com/my-special-image.jpg'}\n      ]\n  },\n\n    components: {\n      appHeader: Header,\n      appFooter: Footer,\n      appLoader: Loader\n    },\n\n    computed: {\n      isLoading() {\n        return this.$store.state.isLoading          \n    }\n    },\n\n    mounted() {\n      console.log(process.env)\n    }\n}\n</script>\n\n<style scoped>\n\n.wrapper {\n  min-height: 100%;\n}\n.slide-fade-enter-active {\n  transition: all .5s ease;\n}\n.slide-fade-leave-active {\n  transition: all .1s cubic-bezier(1.0, 0.5, 0.8, 1.0);\n}\n.slide-fade-enter